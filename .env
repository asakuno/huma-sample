# Database Configuration
DB_HOST=mysql
DB_PORT=3306
DB_USER=user
DB_PASSWORD=password
DB_NAME=database

# AWS Configuration
AWS_REGION=ap-northeast-1
AWS_ACCESS_KEY_ID=dummy_access_key_for_dev
AWS_SECRET_ACCESS_KEY=dummy_secret_key_for_dev

# AWS Cognito Configuration (仮の値 - 開発用)
COGNITO_USER_POOL_ID=ap-northeast-1_XXXXXXXXX
COGNITO_CLIENT_ID=dummy_client_id_for_development_12345678
COGNITO_CLIENT_SECRET=dummy_client_secret_for_development_abcdefghijklmnopqrstuvwxyz1234567890

# JWT Configuration
JWT_SECRET=super-secret-jwt-key-for-development-environment-change-in-production

# Application Configuration
PORT=8888
APP_ENV=development

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:3001
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization,X-Requested-With

# Logging Configuration
LOG_LEVEL=debug
LOG_FORMAT=json

# Admin Configuration (開発用の管理者設定)
ADMIN_EMAILS=admin@example.com,dev@example.com,test@example.com
ADMIN_USERNAMES=admin,root,administrator,dev,developer

# Development Configuration
DEV_MODE=true
MOCK_COGNITO=false
